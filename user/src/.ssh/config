<% if ENV['BEHIND_PROXY'] == '1' %>
# git over SSH to github.com
Host github.com
  User git
  HostName ssh.github.com
  Port 443
<% if ENV['REQUIRE_AUTH'] == '1' %>
  ProxyCommand /usr/bin/corkscrew <%=ENV['PROXY_IP']%> <%=ENV['PROXY_PORT']%> %h %p /home/akira/.ssh/.authorize
<% else %>
  ProxyCommand /usr/bin/corkscrew <%=ENV['PROXY_IP']%> <%=ENV['PROXY_PORT']%> %h %p 
<% end %>  
<% end %>
